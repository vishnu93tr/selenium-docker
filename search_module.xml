<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="search results Duck duck go">
    <listeners>
        <listener class-name="ExtentReports.ExtentReports.ExtentReporterNG"></listener>
    </listeners>
    <test name="Search Test java">
        <parameter name="search_text" value="java"></parameter>
        <classes>
            <class name="com.searchmodule.test.SearchTest"></class>
        </classes>
    </test>
    <test name="Search Test qa">
        <parameter name="search_text" value="qa"></parameter>
        <classes>
            <class name="com.searchmodule.test.SearchTest"></class>
        </classes>
    </test>
    <test name="Search Test docker">
        <parameter name="search_text" value="docker"></parameter>
        <classes>
            <class name="com.searchmodule.test.SearchTest"></class>
        </classes>
    </test>
    <test name="Search Test kubernetes">
        <parameter name="search_text" value="kubernetes"></parameter>
        <classes>
            <class name="com.searchmodule.test.SearchTest"></class>
        </classes>
    </test>
</suite>